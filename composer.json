    "homepage": "https://github.com/yakupbeyoglu/larauto",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5||^8.5"
    },
    "autoload": {
        "psr-4": {
            "Larauto\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larauto\\": "src/"
        }
    },
